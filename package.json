{
  "name": "create-librarydata",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages -d dist",
    "predeploy": "vue-cli-service build",
    "preinstall": "npx npm-force-resolutions",
    "start": "vue-cli-service lint --fix && vue-cli-service serve"
  },
  "dependencies": {
    "@turf/bbox": "^6.5.0",
    "axios": "^0.23.0",
    "colorbrewer": "^1.5.1",
    "core-js": "^3.18.3",
    "file-saver": "^2.0.5",
    "mapbox-gl": "^1.13.2",
    "moment": "^2.29.1",
    "papaparse": "^5.3.1",
    "stream-browserify": "^3.0.0",
    "tableschema": "^1.12.4",
    "vue": "^2.6.14",
    "vue-mapbox": "^0.4.1",
    "vue-markdown-plus": "^2.0.17",
    "vue-router": "^3.5.2",
    "vuetify": "^2.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "^5.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^5.0.0-beta.6",
    "@vue/cli-service": "^5.0.0-beta.6",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.6.0",
    "gh-pages": "^3.1.0",
    "npm-force-resolutions": "^0.0.10",
    "raw-loader": "^4.0.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "^2.4.3",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {
      "vue/valid-v-slot": [
        "error",
        {
          "allowModifiers": true
        }
      ]
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "resolutions": {
    "mapbox-gl": "1.13.1"
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0
  },
  "browser": {
    "fs": false
  }
}
